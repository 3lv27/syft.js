{
  "name": "syft.js",
  "version": "1.4.1",
  "description": "A Javascript port of the PySyft library",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "scripts": {
    "start": "rollup -cw",
    "build": "rollup -c",
    "precommit": "npm run build && pretty-quick --staged",
    "pretest": "npm run build",
    "test": "jest --coverage",
    "test-watch": "jest --watch --coverage",
    "release": "np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenMined/syft.js.git"
  },
  "keywords": [
    "syft",
    "pysyft",
    "openmined",
    "open",
    "mined",
    "deep",
    "learning",
    "private",
    "javascript",
    "machine",
    "learning"
  ],
  "author": "OpenMined",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OpenMined/syft.js/issues"
  },
  "homepage": "https://github.com/OpenMined/syft.js#readme",
  "jest": {
    "testURL": "http://localhost/",
    "verbose": true,
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "dependencies": {
    "@tensorflow/tfjs": "^0.12.5"
  },
  "peerDependencies": {
    "@tensorflow/tfjs": "^0.12.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "mock-socket": "^8.0.2",
    "np": "^3.0.4",
    "prettier": "^1.14.0",
    "pretty-quick": "^1.6.0",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^1.0.1"
  }
}
